{
  "name": "ireporter-react",
  "version": "1.0.0",
  "description": "iReporter enables any/every citizen to bring any form of corruption to the notice of appropriate authorities and the general public. Users can also report on things that needs government intervention",
  "main": "./src/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --config jest.config.js",
    "serve": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --mode development\"",
    "format": "prettier --write \"src/**/*.js\"",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "fix-styles": "prettier-stylelint --write 'src/**/*.{css,scss}'",
    "build": "webpack --mode production",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorduche/ireporter-react.git"
  },
  "keywords": [
    "corruption",
    "report",
    "red-flag",
    "intervention",
    "incident"
  ],
  "author": "Uchenna A. Iheanacho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lorduche/ireporter-react/issues"
  },
  "homepage": "https://github.com/lorduche/ireporter-react#readme",
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "axios": "0.18.0",
    "cross-env": "5.2.0",
    "express": "4.16.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.1",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "semantic-ui-react": "0.86.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.1",
    "dotenv": "7.0.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "imagemin-webpack": "4.1.0",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "mini-css-extract-plugin": "0.6.0",
    "nodemon": "1.18.11",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prettier": "1.16.4",
    "prettier-stylelint": "0.4.2",
    "react-test-renderer": "16.8.6",
    "redux-mock-store": "1.5.3",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "terser-webpack-plugin": "1.2.3",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{css,scss}": [
      "prettier-stylelint --write",
      "git add"
    ]
  },
  "engines": {
    "node": "11.x"
  },
  "prettier": {}
}
